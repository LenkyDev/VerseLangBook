assert{array{1,2}.Remove[0, 2]=array{}}
assert{array{1,2}.Remove[0, 1]=array{2}}
assert{array{1,2}.Remove[1, 2]=array{1}}
assert{array{1,2}.Remove[2, 2]=array{1,2}}
assert{array{   }.Remove[0, 0]=array{}}

assert{array{1,2,3,4,5}.Remove[1, 3]=array{1,4,5}}
assert{array{1,2,3,4,5}.Remove[0, 3]=array{4,5}}
assert{array{1,2,3,4,5}.Remove[3, 5]=array{1,2,3}}
assert{array{1,2,3,4,5}.Remove[1, 4]=array{1,5}}

assert{not array{     }.Remove[0, 1]}
assert{not array{1,2,3}.Remove[1, 0]}
assert{not array{1,2,3}.Remove[-1, 2]}
assert{not array{1,2,3}.Remove[0, 4]}
assert{not array{1,2,3}.Remove[0, -1]}
assert{not array{1,2,3}.Remove[2, 1]}
assert{not array{1,2,3}.Remove[-2, -1]}
assert{not array{1,2,3}.Remove[-1, -2]}
assert{not array{1,2,3}.Remove[1, 4]}
assert{not array{1,2,3}.Remove[-1, 4]}
assert{not array{1,2,3}.Remove[4, -1]}
assert{not array{1,2,3}.Remove[4, 5]}
assert{not array{1,2,3}.Remove[5, 4]}

assert{"12345".Remove[2, 3]="1245"}
assert{"12345".Remove[0, 5]=""}
assert{"12345".Remove[1, 5]="1"}
assert{not "12345".Remove[-1, 5]}