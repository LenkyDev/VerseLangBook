assert:
    X:type = Identity(int)

Identity(X:t where t:type):t = X

class1 := class:

assert:
    X:type = Identity(class1)

struct1 := struct:

assert:
    X:type = Identity(struct1)

assert:
    X:type = Identity([]int)

assert:
    X:type = Identity([int]int)

assert:
    X:type = Identity(?int)

assert:
    X:type = Identity(subtype(class1))

class2(t:type) := class:

assert:
    X:type = Identity(class2(int))

assert:
    X:type = Identity(tuple(int, int))

assert:
    X:type = Identity(int->int)

