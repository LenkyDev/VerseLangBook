# Validate various forms of Continue produces errors in various forms 
assert_syntax_error(3101){"f():void=\{ loop \{continue       \} \}"}
assert_syntax_error(3101){"f():void=\{ loop \{continue 1     \} \}"}
assert_syntax_error(3101){"f():void=\{ loop \{continue(1)    \} \}"}
assert_syntax_error(3101){"f():void=\{ loop \{continue(1)\{\}\} \}"}

