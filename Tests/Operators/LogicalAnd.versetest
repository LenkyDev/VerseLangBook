# Logical `and` operator.
# expr1 and expr2
# Result is expr2 result if both operands succeed or fail if either operand fails.

assert:
    true? and true

assert:
    # equal has higher precedence than `and`
    (1 = 1 and 2) = 2

assert:
    (1 = 1 and 2.0) = 2.0

assert:
    (1 = 1 and false) = false

assert:
    not (false? and false?) = true

assert:
    not (false? and true?) = true

assert:
    not (true? and false?) = true

assert:
    (true? and true?) = false

assert_valid:
    f(index:int):int =
        return (index > 0 and index < 100) or 0
