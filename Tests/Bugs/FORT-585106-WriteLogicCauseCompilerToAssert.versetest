# Start with some asserts that logic works as expected
assert:
    True:logic = logic{true?}
    True?

assert:
    False:logic = logic{false?}
    not (False?)

# More than one expression is combined with 'and'
assert:
    (for (Lhs : array{false, true}, Rhs : array{false, true}, logic{Lhs?, Rhs?} <> logic{Lhs? and Rhs?}) { 42 }).Length = 0

# The expression 'logic{}' triggered an assert in the compiler, this is now an error instead.
# It can be argued that it should be the same as logic{true?} since multiple expressions are combined with ´and´.
assert_semantic_error(3660):
    Failed:logic = logic{}

