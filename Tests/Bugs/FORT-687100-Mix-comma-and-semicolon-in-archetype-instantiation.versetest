abc := class:
    A:int := 1
    B:int := 2
    C:int := 3

# only ; (newline)
assert:
    X := abc{
        B := 20
    }

    X.A = 1
    X.B = 20
    X.C = 3 
    
assert:
    X := abc{
        A := 10
        B := 20
        C := 30
    }

    X.A = 10
    X.B = 20
    X.C = 30 

# only ,
assert:
    X := abc{
        A := 10,
        B := 20
    }

    X.A = 10
    X.B = 20
    X.C = 3

assert:
    X := abc{
        A := 10,
        B := 20,
        C := 30
    }

    X.A = 10
    X.B = 20
    X.C = 30 

assert:
    var Y:int := 0

    X := abc{
        block { set Y = 5 }
        A := 10
        B := 20
        C := 30
    }

    Y = 5
    X.A = 10
    X.B = 20
    X.C = 30 

assert:
    var Y:int := 0

    X := abc{
        block { set Y = 5 },
        A := 10,
        B := 20,
        C := 30
    }

    Y = 5
    X.A = 10
    X.B = 20
    X.C = 30 

# Don't mix ; (newline) and ,
assert_semantic_error(3552):
    abc := class:
        A:int := 1
        B:int := 2
        C:int := 3
    
    F():abc = abc{
        A := 10,
        B := 20
        C := 30
    }

assert_semantic_error(3552):
    abc := class:
        A:int := 1
        B:int := 2
        C:int := 3

    F():abc = abc{
        A := 10
        B := 20,
        C := 30
    }
